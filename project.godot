; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="DND Combat"
run/main_scene="res://2d_test_stage.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GlobalSignals="*res://classes/Global/GlobalSignals.gd"
_Enums="*res://classes/Global/GlobalEnums.gd"
Registry="*res://classes/Resources/Registry.gd"
GlobalBuilder="*res://classes/Global/GlobalBuilder.gd"
GlobalMouseDiceTracker="*res://classes/Global/GlobalMouseDiceTracker.gd"
SkirmishManager="*res://classes/Combat/SkirmishManager.gd"
MultiplayerManager="*res://classes/Global/MultiplayerManager.gd"
Utils="*res://classes/Global/GlobalUtils.gd"
GlobalRPC="*res://classes/Global/GlobalRPC.gd"
Server="*res://classes/Networking/ServerHub.gd"
_CM="*res://classes/Combat/CombatManager2D.gd"

[display]

window/size/always_on_top=true
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

